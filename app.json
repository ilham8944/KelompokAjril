{
  "expo": {
    "name": "Model Atom",
    "slug": "npmAtom",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./images/atom2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./images/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./images/atom2.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.ilhamnugraha.npmAtom"
    },
    "web": {
      "favicon": "./images/atom2.png"
      
    }
  }
}
